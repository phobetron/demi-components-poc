{
  "private": true,
  "name": "library",
  "version": "0.0.0",
  "scripts": {
    "build:vue2": "npx vue-demi-switch 2 vue2 && vite --config ./vite.config.vue2.ts build",
    "build:vue3": "npx vue-demi-switch 3 && vite --config ./vite.config.vue3.ts build",
    "build:switch:vue2": "npx vue-demi-switch 2 vue2",
    "build:switch:vue3": "npx vue-demi-switch 3",
    "typecheck": "vue-tsc --noEmit"
  },
  "files": [
    "dist/**/*"
  ],
  "module": "dist/vue3/index.js",
  "types": "dist/vue3/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3/index.js",
      "types": "./dist/vue3/src/index.d.ts"
    },
    "./vue2": {
      "import": "./dist/vue2/index.js",
      "types": "./dist/vue2/src/index.d.ts"
    },
    "./vue3": {
      "import": "./dist/vue3/index.js",
      "types": "./dist/vue3/src/index.d.ts"
    }
  },
  "dependencies": {
    "vue-demi": "^0.12.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "condition: VUE2 ? : ^2.2.2",
    "@vue/composition-api": "^1.4.6",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-vue2": "condition: VUE2 ? ^1.9.3 :",
    "vite-tsconfig-paths": "^3.3.17",
    "vue": "condition: VUE2 ? ^2.6.14 : ^3.2.31",
    "vue-template-compiler": "condition: VUE2 ? ^2.6.14 :",
    "vue-tsc": "^0.31.4",
    "vue2": "npm:vue@^2.6.14"
  }
}
