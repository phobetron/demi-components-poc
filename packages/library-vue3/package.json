{
  "private": true,
  "name": "library-vue3",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn switch && yarn typecheck && vite build && yarn emit-types",
    "typecheck": "vue-tsc --noEmit",
    "emit-types": "vue-tsc --declaration --emitDeclarationOnly",
    "switch": "npx vue-demi-switch 3"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/tsconfig": "^0.1.3",
    "sass": "^1.49.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vite-tsconfig-paths": "^3.3.17",
    "vue": "^3.2.31",
    "vue-tsc": "^0.31.4"
  }
}
